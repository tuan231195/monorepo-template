{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"tsc": {
			"outputs": []
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": [],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
		},
		"clean": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"watch": {
			"cache": false,
			"outputs": []
		},
		"start": {
			"cache": false,
			"dependsOn": ["build"],
			"outputs": []
		},
		"package": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"release": {
			"cache": false,
			"dependsOn": ["build"],
			"concurrency": 1
		}
	}
}
