{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": [],
	"pipeline": {
		"build": {
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.base.json",
				"../../node_modules/@swc-node/register/package.json",
				"../../node_modules/@swc/cli/package.json",
				"../../node_modules/@swc/core/package.json",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@vdtn359/base-typescript/package.json",
				"../../node_modules/typescript/package.json"
			],
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"tsc": {
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.base.json",
				"../../node_modules/@swc-node/register/package.json",
				"../../node_modules/@swc/cli/package.json",
				"../../node_modules/@swc/core/package.json",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@vdtn359/base-typescript/package.json",
				"../../node_modules/typescript/package.json"
			],
			"outputs": []
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": [],
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"tests/**",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@types/jest/package.json",
				"../../node_modules/@vdtn359/jest-config/package.json",
				"../../node_modules/jest/package.json"
			]
		},
		"clean": {
			"outputs": []
		},
		"lint": {
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"tests/**",
				"../../node_modules/@monorepolint/cli/package.json",
				"../../node_modules/@vdtn359/eslint-config/package.json",
				"../../node_modules/@vdtn359/prettier-config/package.json",
				"../../node_modules/eslint/package.json",
				"../../node_modules/lint-staged/package.json",
				"../../node_modules/monorepolint/package.json",
				"../../node_modules/prettier/package.json"
			],
			"outputs": []
		},
		"watch": {
			"cache": false,
			"outputs": []
		},
		"start": {
			"cache": false,
			"dependsOn": ["build"],
			"outputs": []
		},
		"package": {
			"dependsOn": ["build"],
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.base.json",
				"../../node_modules/@swc-node/register/package.json",
				"../../node_modules/@swc/cli/package.json",
				"../../node_modules/@swc/core/package.json",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@vdtn359/base-typescript/package.json",
				"../../node_modules/typescript/package.json",
				"Dockerfile",
				"../../.dockerignore"
			]
		},
		"patch": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"release": {
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.base.json",
				"../../node_modules/@swc-node/register/package.json",
				"../../node_modules/@swc/cli/package.json",
				"../../node_modules/@swc/core/package.json",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@vdtn359/base-typescript/package.json",
				"../../node_modules/typescript/package.json"
			],
			"cache": false,
			"dependsOn": ["build"]
		},
		"plan-release": {
			"inputs": [
				"src/**",
				"tsconfig.json",
				"package.json",
				"../../tsconfig.base.json",
				"../../node_modules/@swc-node/register/package.json",
				"../../node_modules/@swc/cli/package.json",
				"../../node_modules/@swc/core/package.json",
				"../../node_modules/@swc/jest/package.json",
				"../../node_modules/@vdtn359/base-typescript/package.json",
				"../../node_modules/typescript/package.json"
			],
			"cache": false,
			"dependsOn": ["build"]
		}
	}
}
