{
	"name": "@vdtn359/is-even",
	"version": "0.4.4",
	"main": "dist/index.js",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"lint": "eslint . --max-warnings=0",
		"build": "swc --config-file ../../.swcrc ./src -d dist",
		"test": "jest --passWithNoTests",
		"tsc": "tsc",
		"watch": "swc --config-file ../../.swcrc ./src -d dist -w",
		"clean": "rm -rf dist",
		"release": "release-it --ci",
		"plan-release": "release-it --ci --dry-run",
		"patch": "release-it -i patch --ci",
		"default": "echo default",
		"publish": "pnpm publish --no-git-checks",
		"publish-package": "pnpm publish --no-git-checks"
	},
	"dependencies": {
		"commander": "^8.3.0"
	},
	"types": "src/index.ts"
}
