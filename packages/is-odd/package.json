{
	"name": "@vdtn359/is-odd",
	"version": "0.2.11",
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint . --max-warnings=0",
		"build": "swc --config-file ../../.swcrc ./src -d dist",
		"test": "jest --passWithNoTests",
		"tsc": "tsc",
		"watch": "swc --config-file ../../.swcrc ./src -d dist -w",
