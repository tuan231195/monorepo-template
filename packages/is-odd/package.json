		"release": "release-it --ci",
		"plan-release": "release-it --ci --dry-run",
		"patch": "IGNORE_RECOMMENDED_BUMP=1 release-it -i patch --ci"
	},
	"dependencies": {
		"@vdtn359/is-even": "workspace:*",
		"commander": "^8.3.0"
	},
	"types": "src/index.ts"
}
